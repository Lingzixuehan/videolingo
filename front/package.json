{
  "name": "videolingo-front",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently -k \"tsc -w -p tsconfig.main.json\" \"vite\" \"wait-on http://localhost:5173 && electron ./dist/main/main.js\"",
    "whisper-server": "python ./whisper_server.py",
    "dev:vite": "vite",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "video-server": "node video-server.js"
  },
  "dependencies": {
    "electron": "^39.2.2",
    "express": "^5.1.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/ui": "^4.0.10",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "electron-builder": "^26.0.12",
    "jsdom": "^27.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "wait-on": "^9.0.3"
  }
}
